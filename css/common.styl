.container
  overflow: hidden
  margin: 10px auto
  padding: 0
  width: 750px

.hidden
  display none

@media screen and (max-width: 767px)
  .hidden-m
    display none

.loading
  display: none
  position: fixed
  z-index: 99
  top: 0
  right: 0
  padding: 3px 5px
  background-color: #ffe64d
  color: #000

.error
  display: none
  overflow: hidden
  position: fixed
  z-index: 9999
  left: 0
  top: 0
  width: 100%
  height: 100%
  line-height: 600px
  text-align: center
  background: rgba(0,0,0,0.7)
  color: #f00
  font-weight: bolder

.loading0, .loading0 *
  cursor: progress

.error0, .error0 *
  cursor: not-allowed

.loading0 .loading, .error0 .error
  display: block

