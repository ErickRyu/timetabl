.detail textarea
  width: 99%
  height: 80px
  line-height: 1.5

.detail dl
  min-height: 350px
  -height: 350px
  margin: 10px

.detail dt
  position: absolute
  display: block
  padding: 10px 10px 0
  color: #999

.detail dd
  border-top: 1px solid #eee
  padding: 7px 10px 7px 140px
  line-height: 1.5

.detail del
  color: #999
  text-decoration: none

.pages-cont
  overflow: hidden

.pages
  text-align: center

.pages a
  overflow: hidden
  display: inline-block
  width: 16px
  height: 16px
  font-family: Arial, sans-serif
  color: #3a6e74

.pages a:before
  content: ' '
  overflow: hidden
  display: block
  width: 6px
  height: 6px
  margin: 5px
  border-radius: 3px
  background: #3a6e74

.pages a:hover,
.pages a.self
  color: red

.pages a:hover:before,
.pages a.self:before
  background: red

.search-result
  background: #fff
  border: 1px solid #ccc
  padding-bottom: 15px

.search-result h3
  padding: 0 20px
  line-height: 36px
  font-family: NanumGothic, '나눔고딕', Malgun Gothic, '맑은 고딕', Dotum, '돋움', sans-serif
  font-size: 14px

.search-result table
  margin: 0 auto
  line-height: 24px

.search-result thead
  font-weight: bolder

.search-result td
  padding: 0 10px

.search-result a
  color: #09c

.search-result p
  padding: 15px 20px 0
